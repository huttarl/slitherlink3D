#VRML V2.0 utf8
NavigationInfo {
	type "NONE"
	headlight FALSE
}

EXTERNPROTO Crown_P [ 
eventIn SFVec3f set_point
field SFVec3f point 
eventOut SFVec3f point_changed 
eventOut SFInt32 face_types
eventIn SFInt32 set_visible 
field SFInt32 order
field SFInt32 sub_order
]"crown_proto.wrl#Crown_P"

EXTERNPROTO PolyDisplay [
field SFNode polyhedron 
exposedField MFString projectionURL
]"polydisplay_proto.wrl#PolyDisplay"

Viewpoint {
    position 0 0 10
    fieldOfView 0.3
}

DirectionalLight {
    direction 0.5 -1 -2
    intensity 0.7
    ambientIntensity 0.2
}

DirectionalLight {
    direction -2 0 -2
    intensity 0.7
    ambientIntensity 0.2
}

PolyDisplay {
    polyhedron Crown_P { point 0.5 0.15 0.5  order 5 sub_order 1} 
    projectionURL ["prism10.gif"]
}